import earnix
import pandas as pd
import csv
import numpy as np

driverPath = "D:\\Data\\Ireland\\Earnix Analysis Dataset UAT\\4.UAT_PROD\\Policy_CQ2\\\DriverDateOfBirthBlanks\\Driver_Table.csv"
exportPath = "D:\\Data\\Ireland\\Earnix Analysis Dataset UAT\\4.UAT_PROD\\Policy_CQ2\\\DriverDateOfBirthBlanks\\Blank_DateOfBirth.csv"

df = pd.read_csv(driverPath)

#print(df[df.isnull().any(axis=1)]["dateOfBirth"].head())
df = df[df['dateOfBirth'].isnull()]

df.to_csv (exportPath, index = False, header=True)
